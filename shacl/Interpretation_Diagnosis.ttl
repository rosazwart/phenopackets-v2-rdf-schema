@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix : <https://phenopackets.org> .

:DiagnosisShape
    a sh:NodeShape ;
    sh:targetClass :Diagnosis ;
    sh:property
        [
            sh:path sio:SIO_010299 ;
            sh:class :OntologyClass ;
            sh:name "disease" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "The diagnosed condition." ;
        ],
        [
            sh:path :genomic_interpretations ;
            sh:name "genomic_interpretations" ;
            sh:description "The genomic elements assessed as being responsible for the disease or empty." ;
        ] ;
    sh:comment "The disease that is inferred to be present in the individual or family being analyzed." ;
.