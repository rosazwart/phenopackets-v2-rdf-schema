@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix : <https://phenopackets.org> .

:GenomicInterpretationShape
    a sh:NodeShape ;
    sh:targetClass :GenomicInterpretation ;
    sh:property
        [
            sh:path :subject_or_biosample_id ;
            sh:name "subject_or_biosample_id" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:string;
            sh:description "The id of the patient or biosample that is the subject being interpreted." ;
        ],
        [
            sh:path :interpretation_status ;
            sh:name "interpretation_status" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "Status of the interpretation." ;
        ],
        [
            sh:path :call ;
            sh:xone (
                [
                    sh:class :GeneDescriptor ;
                ]
                [
                    sh:class :VariantInterpretation ;
                ]
            ) ;
            sh:name "call" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "Represents the interpretation." ;
        ] ;
.