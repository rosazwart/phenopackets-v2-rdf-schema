@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix phenop: <https://phenopackets.org/> .

phenop:AcmgPathogenicityClassificationShape
    a sh:NodeShape ;
    sh:targetClass phenop:AcmgPathogenicityClassification ;
    sh:property
        [
            sh:path sio:SIO_000300 ;
            sh:name "has_value" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:string;
            sh:in ("NOT_PROVIDED" "BENIGN" "LIKELY_BENIGN" "UNCERTAIN_SIGNIFICANCE" "LIKELY_PATHOGENIC" "PATHOGENIC") ;
            sh:description "This is an enumeration that describes the pathogenicity classification of variant." ;
        ] ;
.