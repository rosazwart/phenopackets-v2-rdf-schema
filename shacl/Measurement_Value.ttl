@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix : <https://phenopackets.org> .

:ComplexValueShape
    a sh:NodeShape ;
    sh:targetClass :ComplexValue ;
    sh:property
        [
            sh:path :typed_quantities ;
            sh:class :TypedQuantity ;
            sh:name "typed_quantities" ;
            sh:minCount 1 ;
            sh:description "List of quantities required to fully describe the complex value." ;
        ] ;
.

:ValueShape
    a sh:NodeShape ;
    sh:targetClass :Value ;
    sh:property
        [
            sh:path sio:SIO_000300 ;
            sh:or (
                [
                    sh:class :Quantity ;
                ]
                [
                    sh:class :OntologyClass ;
                ]
            ) ;
            sh:name "has_value" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "The outcome (value) of a measurement." ;
        ] ;
.