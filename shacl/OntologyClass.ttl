@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix phenop: <http://purl.org/ejp-rd/phenopackets-rdf-schema/v200/shacl/> .

phenop:ontologyClassShape
	a sh:NodeShape ;
    sh:property
		[
            sh:path rdf:type ;
            sh:minCount 2 ;
            sh:maxCount 2 ;
        ],
		[
            sh:path rdf:type ;
            sh:qualifiedValueShape [ 
                sh:hasValue sio:SIO_000614 ;	# sio:attribute
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
		[
          	sh:path rdf:type ;    
            sh:qualifiedValueShape [ 
                sh:nodeKind sh:IRI ;
                sh:or (
                    [ sh:pattern "^http://www.ebi.ac.uk/efo/" ; ]
                    [ sh:pattern "^http://purl.obolibrary.org/obo/" ; ]
                    [ sh:pattern "^http://www.orpha.net/ORDO/" ; ]
                ) ;
            ] ;
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ]
	;
.
