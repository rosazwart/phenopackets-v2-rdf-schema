@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix : <https://phenopackets.org/> .

:VariantInterpretationShape
    a sh:NodeShape ;
    sh:targetClass :VariantInterpretation ;
    sh:property
        [
            sh:path obo:NCIT_C168798 ;
            sh:class :AcmgPathogenicityClassification ;
            sh:name "acmg_pathogenicity_classification" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "One of the five ACMG pathogenicity categories, or NOT_PROVIDED." ;
        ],
        [
            sh:path :therapeutic_actionability ;
            sh:class :TherapeuticActionability ;
            sh:name "therapeutic_actionability" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "The therapeutic actionability of the variant, default is UNKNOWN_ACTIONABILITY." ;
        ],
        [
            sh:path :variation_descriptor ;
            sh:class :VariationDescriptor ;
            sh:name "variation_descriptor" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "A genetic/genomic variant." ;
        ] ;
.