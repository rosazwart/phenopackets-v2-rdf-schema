@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix : <https://phenopackets.org> .

:TherapeuticActionabilityShape
    a sh:NodeShape ;
    sh:targetClass :TherapeuticActionability ;
    sh:property
        [
            sh:path sio:SIO_000300 ;
            sh:name "has_value" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:string;
            sh:in ("UNKNOWN_ACTIONABILITY" "NOT_ACTIONABLE" "ACTIONABLE") ;
            sh:description "This is an enumeration that describes the therapeutic actionability for this variant." ;
        ] ;
.