@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix phenop: <http://purl.org/ejp-rd/phenopackets-rdf-schema/v200/shacl/> .

phenop:individualShape
    a sh:NodeShape ;
    sh:property
		[
            sh:path rdf:type ;
            sh:minCount 2 ;
            sh:maxCount 2 ;
        ],
		[
            sh:path rdf:type ;
            sh:qualifiedValueShape [ 
                sh:hasValue sio:SIO_010375 ;	# sio:multi-cellular-organism
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
		[
            sh:path rdf:type ;
            sh:qualifiedValueShape [ 
                sh:hasValue obo:NCIT_C41189 ;	# obo:study-subject
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
        [
            sh:path sio:SIO_000228 ;    # sio:has-role
            sh:name "sio:has-role" ;
            sh:node phenop:roleShape ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
        ]
.