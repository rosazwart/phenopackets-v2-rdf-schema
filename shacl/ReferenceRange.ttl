@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix : <https://phenopackets.org> .

:ReferenceRangeShape
    a sh:NodeShape ;
    sh:targetClass :ReferenceRange ;
    sh:property
        [
            sh:path sio:SIO_000074 ;
            sh:class :OntologyClass ;
            sh:name "unit" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "Ontology term describing the unit." ;
        ],
        [
            sh:path :low ;
            sh:name "low" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:double;
            sh:description "Lower range of normal." ;
        ],
        [
            sh:path :high ;
            sh:name "high" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:double;
            sh:description "Upper range of normal." ;
        ] ;
.