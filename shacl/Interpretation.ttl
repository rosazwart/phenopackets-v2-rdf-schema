@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix : <https://phenopackets.org> .

:InterpretationShape
    a sh:NodeShape ;
    sh:targetClass :Interpretation ;
    sh:property
        [
            sh:path sio:SIO_000115 ;
            sh:name "id" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:string;
            sh:description "Arbitrary identifier." ;
        ],
        [
            sh:path :progress_status ;
            sh:class :ProgressStatus ;
            sh:name "progress_status" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:description "The current resolution status." ;
        ],
        [
            sh:path sio:SIO_001331 ;
            sh:name "diagnosis" ;
            sh:minCount 0 ;
            sh:maxCount 1 ;
            sh:description "The diagnosis, if made." ;
        ],
        [
            sh:path :summary ;
            sh:name "summary" ;
            sh:minCount 0 ;
            sh:maxCount 1 ;
            sh:datatype xsd:string;
            sh:description "Additional data about this interpretation." ;
        ] ;
.