phenop:chromosomeShape
    a sh:NodeShape ;
    sh:targetClass obo:NCIT_C13202 ;    # obo:chromosome
    sh:property
		[
            sh:path rdf:type ;
            sh:hasValue sio:SIO_000614 ;    # sio:attribute
        ],
		[
          	sh:path dct:identifier ;    
            sh:name "dct:identifier" ;
            sh:datatype xsd:string ;
