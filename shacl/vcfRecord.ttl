# TODO: find better ontology term for vcf record
phenop:vcfRecordShape
    a sh:NodeShape ;
    sh:targetClass obo:NCIT_C172216 ;
    sh:property
		[
            sh:path rdf:type ;
            sh:minCount 2 ;
            sh:maxCount 2 ;
        ],
		[
            sh:path rdf:type ;
            sh:qualifiedValueShape [ 
                sh:hasValue sio:SIO_000015 ;	# sio:information-content-entity
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
		[
            sh:path rdf:type ;
            sh:qualifiedValueShape [ 
                sh:hasValue obo:NCIT_C172216 ;	# obo:variant-call-format
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
        [
            sh:path sio:SIO_000008 ;    # sio:has-attribute
            sh:name "sio:has-attribute" ;
            sh:or (
                [ sh:class obo:NCIT_C164815 ; ] # obo:reference-genome-assembly-version 
                [ sh:class obo:NCIT_C164716 ; ] # obo:chromosome-name
                [ sh:class obo:NCIT_C104788 ; ] # obo:position
                [ sh:class obo:NCIT_C164388 ; ] # obo:genetic-reference-sequence
                [ sh:class obo:NCIT_C25415 ; ]  # obo:alternative
            ) ;
            sh:minCount 5 ;
        ],
        [
            sh:path sio:SIO_000008 ;    # sio:has-attribute
            sh:qualifiedValueShape [ 
                sh:class obo:NCIT_C164815 ;    # obo:reference-genome-assembly-version
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
        [
            sh:path sio:SIO_000008 ;    # sio:has-attribute
            sh:qualifiedValueShape [ 
                sh:class obo:NCIT_C164716 ;    # obo:chromosome-name
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
        [
            sh:path sio:SIO_000008 ;    # sio:has-attribute
            sh:qualifiedValueShape [ 
                sh:class obo:NCIT_C104788 ;    # obo:position
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
        [
            sh:path sio:SIO_000008 ;    # sio:has-attribute
            sh:qualifiedValueShape [ 
                sh:class obo:NCIT_C164388 ;    # obo:genetic-reference-sequence
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ],
        [
            sh:path sio:SIO_000008 ;    # sio:has-attribute
            sh:qualifiedValueShape [ 
                sh:class obo:NCIT_C25415 ;    # obo:alternative
            ];
           	sh:qualifiedMinCount 1 ;
           	sh:qualifiedMaxCount 1 ;
        ]
    ;
.