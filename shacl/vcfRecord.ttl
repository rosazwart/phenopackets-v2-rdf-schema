phenop:genomeAssemblyShape
    a sh:NodeShape ;
    sh:targetClass obo:NCIT_C73517 ;    # obo:genome-assembly-sequence 
    sh:property
		[
            sh:path rdf:type ;
            sh:hasValue sio:SIO_000614 ;    # sio:attribute
        ],
		[
          	sh:path dct:identifier ;    
            sh:name "dct:identifier" ;
            sh:datatype xsd:string ;
           	sh:minCount 1 ;
           	sh:maxCount 1 ;
        ]
    ;
