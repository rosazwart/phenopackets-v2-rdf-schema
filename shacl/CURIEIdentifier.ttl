@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix : <https://phenopackets.org> .

:CURIEIdentifierShape
    a sh:NodeShape ;
    sh:targetClass :CURIEIdentifier ;
    sh:property
        [
            sh:path sio:SIO_000300 ;
            sh:name "has_value" ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:datatype xsd:string;
            sh:in (
                [
                    sh:pattern "^[^:]+:[^:]+$" ;
                ]
            ) ;
            sh:description "A W3C Compact URI formatted string. A CURIE string has the structure prefix:reference, as defined by the W3C syntax." ;
        ] ;
.